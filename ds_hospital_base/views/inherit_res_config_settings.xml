<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        
        <record id="res_config_settings_ds_hospital_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.ds.hospital.view.form</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="inside">
                    <app data-string="Hospital" string="Hospital" name="ds_hospital_base" groups="ds_hospital_base.group_hospital_access_manager">
                        <h2>Base Hospital</h2>
                        <div id="hospital_base" class="row mt16 o_settings_container">
                            <div class="col-12 col-lg-6 o_setting_box">
                                <div class="o_setting_left_pane"></div>
                                <div class="o_setting_right_pane">
                                    <span class="o_form_label">Auto Invoice</span>
                                    
                                    <div class="text-muted">
                                        <field name="auto_invoice" string="Auto Invoice"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </app>
                </xpath>
            </field>
        </record>
    </data>
    

</odoo>
