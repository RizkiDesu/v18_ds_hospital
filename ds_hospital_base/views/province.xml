<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    <!-- ds.province tree view otree -->
    <record id="ds_province_view_tree" model="ir.ui.view">
        <field name="name">ds.province.view.tree</field>
        <field name="model">ds.province</field>
        <field name="arch" type="xml">
            <list>
                <!-- Add your fields here ofield -->
                <field name="name"/>
                <field name="description"/>
                <field name="mobile_number"/>
                <field name="insurance_number"/>
                <field name="insurance_type"/>
            </list>
        </field>
    </record>
        <!-- ds.province action window -->
        <record id="ds_province_action" model="ir.actions.act_window">
            <field name="name">ds.province.action</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ds.province</field>
            <field name="view_mode">list,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p><p>
                    <!-- More details about what a user can do with this object will be OK --> 
                
                </p>
            </field>
        </record>
    <!-- This Menu Item must have a parent and an action -->
    <menuitem id="ds_province_menu_action" name="province" parent="ds_area_menu_categ" action="ds_province_action" sequence="0"/>
    </data>
    

</odoo>
